<?xml version='1.0' encoding='utf-8'?>
<widget id="com.catchtime.web" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>어바웃캐치타임</name>
    <description>
        About Catch Time
    </description>
    <author email="info@catchtime.co.kr" href="https://catchtime.co.kr">
        CatchTime Team
    </author>
    <content src="index.html" />
    <splash src="www/res/screen/android/screen.png" />
    <access origin="https://catchtime.co.kr" subdomains="true" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="FadeSplashScreenDuration" value="1000" />
</widget>
